# Configuração para testes de API A2A

# URL do servidor A2A para testes
A2A_TEST_URL=http://localhost:8000

# Timeout para requisições (segundos)
A2A_TEST_TIMEOUT=30

# Configuração de autenticação (se necessário)
# A2A_TEST_API_KEY=your_api_key_here
# A2A_TEST_TOKEN=your_token_here

# Configuração de debug
A2A_TEST_DEBUG=false

# Configuração específica do agente
A2A_TEST_AGENT_NAME=helloworld
A2A_TEST_AGENT_VERSION=1.0.0